package backjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		float sum = 0;
		int count = 0;
		for (int i = 0; i < 20; i++) {
			sc.next();
			float num = sc.nextFloat();
			String grade = sc.next();

			System.out.println(grade);
			if (grade.equals("P") || grade.equals("F")) {
				continue;
			}
			float gradeNum = 0;
			switch (grade.charAt(0)) {
			case 'A':
				gradeNum = 4;
				break;
			case 'B':
				gradeNum = 3;
				break;
			case 'C':
				gradeNum = 2;
				break;
			case 'D':
				gradeNum = 1;
				break;
			case 'F':
				gradeNum = 0;
				break;
			}

			if (grade.charAt(1) == '+')
				gradeNum += 0.5;
			sum += gradeNum * num;
			count++;
			System.out.println(gradeNum);
		}
		
		System.out.printf("%.6f", sum / count);
		sc.close();
	}

}
