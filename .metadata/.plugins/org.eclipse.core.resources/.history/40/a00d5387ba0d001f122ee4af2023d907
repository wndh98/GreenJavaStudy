package cs01_if;

import java.util.Scanner;

public class If08 {

	public static void main(String[] args) {

		Scanner sc= new Scanner(System.in);
		
		int month;
		String season;
		System.out.println("몇 월생 입니까? ");
		
		month=sc.nextInt();
		
		System.out.printf("%d월에 태어난 당신은 사랑스런 %s의 선물", month, season);
		sc.close();
	}

}
