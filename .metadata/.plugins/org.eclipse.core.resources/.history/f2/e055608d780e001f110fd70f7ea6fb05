package cs03_for;

public class For11 {

	public static void main(String[] args) {
		// 1~100ê¹Œì§€ì˜ í•©ê³„ ì¶œë ¥
		// ì¶œë ¥ì˜ ì˜ˆ
		// 1 ~ 10ê¹Œì§€ì˜ í•©: 55
		// 11 ~ 20ê¹Œì§€ì˜ í•©: 155(1~20ê¹Œì§€ì˜ í•© - 1~10ê¹Œì§€ì˜ í•© )~~

		int sum = 0;
		int preSum = 0;
		for (int i = 1; i <= 100; i++) {
			sum += i;
			if (i % 10 == 0) {
				System.out.println(i - 9 + "ë¶€í„° " + i + "ê¹Œì§€ì˜ í•©: " + (sum - preSum));
				preSum = sum;
			}
		}
	}

}

