package backjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		float sum = 0;
		float avg=0;
		for (int i = 0; i < 20; i++) {
			sc.next();
			float num = sc.nextFloat();
			String grade = sc.next();

			if (grade.equals("P") || grade.equals("F")) {
				continue;
			}
			float gradeNum = 0;
			switch (grade.charAt(0)) {
			case 'A':
				gradeNum = 4;
				break;
			case 'B':
				gradeNum = 3;
				break;
			case 'C':
				gradeNum = 2;
				break;
			case 'D':
				gradeNum = 1;
				break;
			case 'F':
				gradeNum = 0;
				break;
			}

			if (grade.charAt(1) == '+')gradeNum += 0.5;
			
			sum += num;
			avg+=gradeNum*num;
			
		}
		if(avg>0) {
			System.out.printf("%.6f", avg / sum);
		}else {
			System.out.printf("%.6f", (float)0.0);
		}
		sc.close();
	}

}
