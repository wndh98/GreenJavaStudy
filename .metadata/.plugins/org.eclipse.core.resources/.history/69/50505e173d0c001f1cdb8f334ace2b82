package variable;

public class Var03 {
	
	static int num=10;
	
	public static void main(String[] args) {
//		º¯¼öÀÇ scope
		int num1 = 50;
		System.out.println(num);
		System.out.println(num1);
		if(true) {
			int num2=10;
			System.out.println("num1" + num1);
			System.out.println("num1" + num2);
		}
		System.out.println("num1" + num1);
		System.out.println("num1" + num2);
		
		
	}

}
