package backjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int[][] arr1=new int[9][9];
		int N=0,M=0;
		for(int i=0;i<9;i++) {
			for(int j=0;j<9;j++) {
				arr1[i][j]=sc.nextInt();
				if(arr1[i][j]>arr1[N][M]) {
					N=i;
					M=j;
				}
			}
		}
		System.out.println(arr1[N][M]);
		System.out.println((N+1) + " " + (M+1));

		sc.close();
	}

}
