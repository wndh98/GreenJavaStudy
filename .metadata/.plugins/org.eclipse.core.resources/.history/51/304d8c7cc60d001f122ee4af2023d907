package backjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String[] arr=new String[5];
		int max=0;
		for(int i=0;i<5;i++) {
			arr[i]=sc.next();
			if(arr[i].length()>max) {max=i;}
		}
		for(int i=0;i<max;i++) {
			
		}
		sc.close();
	}

}


