package cs02_switch;

import java.util.Scanner;

public class Sw03 {

	public static void main(String[] args) {
		int month;
		String season = "";
		System.out.println("태어난 달을 입력하세요>>> ");
		Scanner sc = new Scanner(System.in);
		month = sc.nextInt();
		switch (month) {
		case 12:
			season = "겨울";
			break;
		case 11:
			season = "가을";
			break;
		case 10:
			season = "가을";
			break;
		case 9:
			season = "가을";
			break;
		case 8:
			season = "여름";
			break;
		case 7:
			season = "여름";
			break;
		case 6:
			season = "여름";
			break;
		case 5:
			season = "봄";
			break;
		case 4:
			season = "봄";
			break;
		case 3:
			season = "봄";
			break;
		case 2:
			season = "겨울";
			break;
		case 1:
			season = "겨울";
			break;
		default:
		}
		if (month <= 12 && month >= 1) {
			System.out.printf("%d월에 태어난 당신은 사랑스런 %s의 선물", month, season);
		}
		sc.close();
	}
}

// 지리죠?
