package ch03_operator;

public class Op03 {
	// 논리 연산자: 비교연산자보다 순위가 뒤
	// &&(and), ||(or), !(not), ^(xor)

	public static void main(String[] args) {
		int n1 = 10, n2 = 4, n3 = 7, n4 = 15;
		// &&: 좌항, 우항이 모두 참일 경우에만 참
		// ||: 좌항, 우항 어느 한쪽이라도 참이면 참
		
		//^: 좌항, 우항의 값이 같으면 false, 다르면 true
		System.out.println(n1>n2 ^ n3 > n4);
		
	}

}
