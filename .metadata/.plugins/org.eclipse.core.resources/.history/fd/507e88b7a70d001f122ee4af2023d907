package ch03_operator;

public class Op11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String a = "I ";
		String b = "love ";
		String c = "you.";
		String d = a + b + c;
		System.out.println(d);
		
		String e = "Java";
		System.out.println(e + " " + 1.8);
		System.out.println(e *3);
		
		
	}

}
